{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@dapp/*": ["./src/dapp/*"],
      "@styles/*": ["./src/styles/*"],
      "@assets/*": ["./src/assets/*"],

    }
  },
  "include": ["src", "vite.config.ts", "project.ts"],
  "exclude": ["node_modules", "dist"]
}




